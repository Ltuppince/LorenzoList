<div class="container">
    <div class="row">
        <select class="uk-select col-lg-4 categorySelect">
            <option>Choose Category</option>
        </select>
    </div>

    <div class="row">
        {{#each items}}
        <div class="uk-grid-match col-lg-4 col-md-4 col-sm-6 itemsContainer" data-id="{{dataValues.id}}" data-cat="{{dataValues.CategoryId}}" uk-grid>
            <div class="itemCard" id="{{dataValues.id}}">
                <div class="uk-card uk-card-default uk-card-hover uk-card-body" id="{{dataValues.id}}">
                    <h3 class="uk-card-title" id="{{dataValues.id}}">{{dataValues.title}}</h3>
                    <p id="{{dataValues.id}}">${{dataValues.price}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>